{
  "entrypoint": "index.html",
  "shell": "src/my-app.html",
  "fragments": [
    "src/my-view1.html",
    "src/my-view2.html",
    "src/my-view3.html",
    "src/my-sign-message.html",
    "src/my-verify-message.html",
    "src/my-dashboard.html",
    "src/my-create-wallet.html",
    "src/my-view404.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js",
    "socket.io.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "preset": "es6-unbundled",
      "basePath": true
    },
    {
      "preset": "es6-bundled",
      "basePath": true,
      "bundle": {"excludes": ["socket.io.js"]}
    },
    {
      "preset": "es5-bundled",
      "basePath": true,
      "bundle": {"excludes": ["socket.io.js"]}
    }
  ]
}
